<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find braccio_camera_control)/config/braccio_camera_control.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="camera_controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
	output="screen" ns="/braccio_camera" args=" braccio_camera_base_joint_position_controller 
                                                braccio_camera_base_depth_position_controller 
                                                braccio_camera_base_height_position_controller 
                                                braccio_camera_tilt_position_controller 
                                                braccio_camera_state_controller"/>
  

  <node pkg="robot_state_publisher" type="robot_state_publisher" name="braccio_camera_tf_publisher" >
      <remap from="robot_description" to="robot_description" />
      <remap from="joint_states" to="/braccio_camera/joint_states" />
    </node>    
  
  
</launch>
