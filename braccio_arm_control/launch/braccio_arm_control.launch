<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find braccio_arm_control)/config/braccio_arm_control.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
	output="screen" ns="/braccio_arm" args="base_joint_position_controller 
        shoulder_joint_position_controller elbow_joint_position_controller wrist_pitch_joint_position_controller
        wrist_roll_joint_position_controller gripper_joint_position_controller sub_gripper_joint_position_controller braccio_arm_state_controller"/>
  

  <node pkg="robot_state_publisher" type="robot_state_publisher" name="braccio_tf_publisher" >
      <remap from="robot_description" to="robot_description" />
      <remap from="joint_states" to="/braccio_arm/joint_states" />
    </node>    
  
  
</launch>
